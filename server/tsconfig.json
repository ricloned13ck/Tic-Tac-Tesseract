{
  "compilerOptions": {
    "outDir": "./server/dist",
    "rootDir": "./server",
    "module": "commonjs",
    "target": "ES2020",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false
  },
  "include": ["server/**/*.ts"]
}
